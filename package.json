{
  "name": "@open-core/npc",
  "version": "0.1.0",
  "description": "Official NPC + AI plugin for OpenCore",
  "license": "MPL-2.0",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js",
      "require": "./dist/server/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js",
      "require": "./dist/client/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "typecheck": "tsc -p tsconfig.build.json --noEmit && tsc -p tsconfig.test.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "@open-core/framework": "*"
  },
  "dependencies": {
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@open-core/framework": "file:../opencore-framework",
    "@citizenfx/client": "2.0.25963-1",
    "@citizenfx/server": "2.0.25963-1",
    "@types/node": "^24.5.2",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "packageManager": "pnpm@10.28.0"
}
